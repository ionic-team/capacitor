{
  "name": "@capacitor/cli",
  "version": "1.0.0-alpha.34",
  "description": "Capacitor: Cross-platform apps with JavaScript and the web",
  "homepage": "https://capacitor.ionicframework.com/",
  "author": "Ionic Team <hi@ionicframework.com> (https://ionicframework.com) ",
  "bin": {
    "capacitor": "./bin/capacitor",
    "cap": "./bin/capacitor"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run assets && tsc",
    "clean": "rm -rf ./dist",
    "deploy": "np",
    "lint": "tslint --project tsconfig.json",
    "assets": "node scripts/copy-assets.js",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "watch": "npm run assets && tsc -w"
  },
  "files": [
    "assets/",
    "bin/",
    "dist/"
  ],
  "keywords": [
    "ionic",
    "ionic framework",
    "capacitor",
    "universal app",
    "progressive web apps",
    "cross platform"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/capacitor/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/dev-server": "0.0.18-1",
    "@types/tar": "^4.0.0",
    "chalk": "^2.3.0",
    "commander": "^2.12.2",
    "compare-versions": "^3.1.0",
    "fs-extra": "^4.0.3",
    "inquirer": "^4.0.1",
    "opn": "^5.1.0",
    "ora": "^1.3.0",
    "semver": "^5.4.1",
    "superagent": "^3.8.2",
    "superagent-proxy": "^1.0.2",
    "tar": "^4.2.0",
    "which": "^1.3.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/fs-extra": "^4.0.5",
    "@types/inquirer": "0.0.35",
    "@types/jest": "^21.1.10",
    "@types/mock-fs": "^3.6.30",
    "@types/opn": "^5.1.0",
    "@types/ora": "^1.3.1",
    "@types/semver": "^5.4.0",
    "@types/which": "^1.0.28",
    "@types/xml2js": "^0.4.2",
    "jest": "^21.2.1",
    "mock-fs": "^4.4.2",
    "np": "^2.18.2",
    "tmp": "0.0.33",
    "ts-jest": "^21.2.4",
    "tslint": "^5.8.0",
    "tslint-ionic-rules": "0.0.11",
    "typescript": "^2.6.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/util.ts"
    ]
  }
}
