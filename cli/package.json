    "test": "jest -i",
    "watch": "npm run assets && tsc -w"
  },
  "dependencies": {
    "@ionic/cli-framework-output": "^2.2.1",
    "@ionic/utils-fs": "^3.1.5",
    "@ionic/utils-subprocess": "^2.1.6",
    "@ionic/utils-terminal": "^2.3.0",
    "commander": "^6.0.0",
    "debug": "^4.2.0",
    "env-paths": "^2.2.0",
    "kleur": "^4.1.1",
    "native-run": "^1.2.1",
    "open": "^8.0.2",
    "plist": "^3.0.1",
    "prompts": "^2.3.2",
    "semver": "^7.3.2",
    "tar": "^6.0.5",
    "tslib": "^2.1.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.4",
    "@types/open": "^6.1.0",
    "@types/plist": "^3.0.2",
    "@types/prompts": "^2.0.8",
    "@types/semver": "^7.3.1",
    "@types/slice-ansi": "^4.0.0",
    "@types/tar": "^4.0.4",
    "@types/tmp": "^0.2.0",
    "@types/xml2js": "0.4.5",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "tmp": "^0.2.1",
    "ts-jest": "^26.1.3",
    "typescript": "~4.1.2"
