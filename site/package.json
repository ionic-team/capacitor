{
  "name": "capacitor-site",
  "private": true,
  "version": "0.0.1",
  "description": "Capacitor Site",
  "scripts": {
    "docs": "gulp",
    "build": "gulp && bash scripts/docs.sh && stencil build --prerender",
    "dev": "gulp && sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "deploy": "bash scripts/deploy.sh",
    "serve": "stencil-dev-server",
    "start": "rm -rf www && npm run build && npm run dev"
  },
  "dependencies": {
    "@stencil/router": "0.0.25",
    "@stencil/sass": "0.0.3"
  },
  "devDependencies": {
    "@stencil/core": "0.6.18",
    "@stencil/dev-server": "0.0.18",
    "@stencil/utils": "latest",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-markdown": "^2.0.1",
    "gulp-rename": "^1.2.2",
    "highlight.js": "^9.12.0"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/capacitor"
  },
  "homepage": "https://github.com/ionic-team/capacitor"
}
